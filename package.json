{
    "name": "inline-sql-syntax",
    "displayName": "Inline SQL Syntax",
    "description": "Highlight strings as SQL. Python, Go, JS, TS, Ruby, Java, C#.",
    "version": "2.4.1",
    "publisher": "qufiwefefwoyn",
    "icon": "docs/logo.png",
    "repository": {
        "url": "https://github.com/barklan/inline_sql_syntax"
    },
    "engines": {
        "vscode": "^1.32.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "grammars": [
            {
                "path": "./syntaxes/highlight-sql-string.json",
                "scopeName": "inline-sql.injection",
                "injectTo": [
                    "source.python",
                    "source.go",
                    "source.java",
                    "source.ruby",
                    "source.cs",
                    "source.js",
                    "source.ts",
                    "source.vue",
                    "source.svelte",
                    "source.tsx"
                ],
                "embeddedLanguages": {
                    "meta.embedded.sql": "sql"
                }
            }
        ]
    }
}
