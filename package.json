{
    "name": "inline-sql-syntax",
    "displayName": "Inline SQL",
    "description": "Highlight and lint SQL strings. Python, Go, JS, TS, Ruby, Java, C#.",
    "version": "2.6.0",
    "publisher": "qufiwefefwoyn",
    "icon": "docs/logo.png",
    "repository": {
        "url": "https://github.com/barklan/inline_sql_syntax"
    },
    "engines": {
        "vscode": "^1.32.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "activationEvents": [
        "onLanguage:python",
        "onLanguage:go",
        "onLanguage:js",
        "onLanguage:ts",
        "onLanguage:java",
        "onLanguage:ruby",
        "onLanguage:cs"
    ],
    "main": "./out/extension.js",
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "lint": "eslint . --ext .ts,.tsx",
        "watch": "tsc -watch -p ./"
    },
    "contributes": {
        "grammars": [
            {
                "path": "./syntaxes/highlight-sql-string.json",
                "scopeName": "inline-sql.injection",
                "injectTo": [
                    "source.python",
                    "source.go",
                    "source.java",
                    "source.ruby",
                    "source.cs",
                    "source.js",
                    "source.ts"
                ],
                "embeddedLanguages": {
                    "meta.embedded.sql": "sql"
                }
            }
        ]
    },
    "devDependencies": {
        "@types/node": "^17.0.21",
        "@types/vscode": "^1.65.0",
        "@typescript-eslint/eslint-plugin": "^5.13.0",
        "@typescript-eslint/parser": "^5.13.0",
        "eslint": "^8.10.0",
        "typescript": "^4.6.2"
    },
    "dependencies": {
        "sql-lint": "^0.0.19"
    }
}
