{
  "fileTypes": [
    "py", "go", "js", "ts", "java", "rb", "cs"
  ],
  "injectionSelector": "L:source -comment -string",
  "patterns": [
    {
      "begin": "(`)((--sql)|SELECT |INSERT INTO |DELETE |UPDATE |CREATE TABLE)",
      "beginCaptures": {
        "2": {
          "name": "keyword"
        }
      },
      "end": "(`)",
      "patterns": [
        {
          "include": "source.sql"
        }
      ]
    },
    {
      "begin": "(\")((--sql)|SELECT |INSERT INTO |DELETE |UPDATE |CREATE TABLE)",
      "beginCaptures": {
        "2": {
          "name": "keyword"
        }
      },
      "end": "(\")",
      "patterns": [
        {
          "include": "source.sql"
        }
      ]
    },
    {
      "begin": "(\"\"\")((--sql)|SELECT |INSERT INTO |DELETE |UPDATE |CREATE TABLE)",
      "beginCaptures": {
        "2": {
          "name": "keyword"
        }
      },
      "end": "(\"\"\")",
      "patterns": [
        {
          "include": "source.sql"
        }
      ]
    }
  ],
  "scopeName": "inline-sql.injection"
}
